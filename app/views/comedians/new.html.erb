<%= form_for @comedians do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <%= f.label :birthplace %>
  <%= f.text_field :birthplace %>
  <%= f.label :image_url %>
  <%= f.text_field :image_url %>

  <%= f.submit 'Create Comedian' %>
<% end %>
